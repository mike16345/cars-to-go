model Car {
  id        String   @id @default(uuid())
  make      String
  model     String
  year      Int
  mileage   Int?      
  price     Int
  status    CarStatus
  description String?
  images    String[]
  featured  Boolean @default(false)
  createdAt DateTime @default(now())
}

enum CarStatus {
  AVAILABLE
  COMING_SOON
  SOLD
}